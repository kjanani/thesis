	Journal = {J Public Health Manag Pract},
	Journal-Full = {Journal of public health management and practice : JPHMP},
	Month = {Sep},
	Pmid = {26334537},
	Pst = {aheadofprint},
	Title = {Local Health Departments Tweeting About Ebola: Characteristics and Messaging},
	Year = {2015},
	Bdsk-Url-1 = {http://dx.doi.org/10.1097/PHH.0000000000000342}}

