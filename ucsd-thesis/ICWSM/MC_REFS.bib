	Mesh = {Attitude to Health; Centers for Disease Control and Prevention (U.S.); Disease Outbreaks; Humans; Immunization; Social Media; United States},
	Month = {Oct},
	Number = {10},
	Pages = {e1002199},
	Pmc = {PMC3192813},
	Pmid = {22022249},
