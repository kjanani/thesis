	Doi = {10.1197/jamia.M2544},
	Journal = {J Am Med Inform Assoc},
	Journal-Full = {Journal of the American Medical Informatics Association : JAMIA},
	Mesh = {Communicable Diseases; Disease Outbreaks; Humans; Internet; Mass Media; Population Surveillance; Reproducibility of Results; Software},
	Number = {2},
	Pages = {150-7},
	Pmc = {PMC2274789},
